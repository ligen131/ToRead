    "@types/react-dom": "^19.1.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^3.0.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.3"
  }
}
