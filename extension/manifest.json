{
  "manifest_version": 3,
  "name": "ToRead",
  "version": "1.0",
  "description": "Save current page to ToRead collection",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/page-ai-line.png",
      "48": "images/page-ai-line.png",
      "128": "images/page-ai-line.png"
    }
  },
  "icons": {
    "16": "images/page-ai-line.png",
    "48": "images/page-ai-line.png",
    "128": "images/page-ai-line.png"
  }
}
